    
{
    "name": "imiphp/imi-amqp",
    "type": "library",
    "license": "MIT",
    "description": "支持在 imi 框架中使用 支持 AMQP 协议的消息队列，如：RabbitMQ",
    "require": {
        "php-amqplib/php-amqplib": "^2.11.0"
    },
    "require-dev": {
        "yurunsoft/imi": "dev-dev",
        "yurunsoft/yurun-http": "^3.4.0",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4" : {
            "Imi\\AMQP\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "ImiApp\\" : "example/",
            "Imi\\AMQP\\Test\\": "tests/unit/"
        }
    },
    "scripts": {
      "test": "./tests/run"
    }
}